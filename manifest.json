{
  "name": "Cafe Club TV - Entretenimiento",
  "short_name": "CafeClubTV",
  "description": "Bienvenido a CafeClubTV, tu destino para disfrutar de los mejores videos y playlists de entretenimiento de calidad.",
  "start_url": "/",
  "scope": "/",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#8B0000",
  "icons": [
    {
      "src": "/images/Icon192x192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "/images/Icon512x512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "id": "https://www.cafeclubtv.com/",
  "lang": "es",
  "dir": "ltr",
  "orientation": "portrait",
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "scope_extensions": [
    {
      "origin": "https://videos.cafeclubtv.com",
      "path": "/"
    },
    {
      "origin": "https://media.cafeclubtv.com",
      "path": "/content/"
    }
  ],
  "display_override": [
    "window-controls-overlay",
    "standalone"
  ],
  "related_applications": [
    {
      "platform": "play",
      "id": "com.ejemplo.aplicacion1"
    },
    {
      "platform": "itunes",
      "id": "id123456789"
    },
    {
      "platform": "windows",
      "id": "com.ejemplo.aplicacion2"
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+videos",
      "url": "https://www.cafeclubtv.com/view?video=%s"
    },
    {
      "protocol": "web+playlist",
      "url": "https://www.cafeclubtv.com/playlist?name=%s"
    }
  ],
  "categories": [
    "entertainment",
    "video",
    "media"
  ],
  "screenshots": [
    {
      "src": "/images/screenshot1.png",
      "sizes": "1280x800",
      "type": "image/png",
      "description": "Pantalla principal de CafeClubTV"
    },
    {
      "src": "/images/screenshot2.png",
      "sizes": "1280x800",
      "type": "image/png",
      "description": "Sección de playlists"
    },
    {
      "src": "/images/screenshot3.png",
      "sizes": "1280x800",
      "type": "image/png",
      "description": "Sección de videos cortos"
    }
  ]
}